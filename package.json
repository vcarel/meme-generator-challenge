{
  "name": "frontend-technical-test-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check --error-on-warnings",
    "lintfix": "biome check --error-on-warnings --fix",
    "check-types": "tsc -b --incremental && echo -e \"Typescript validation \\033[0;32mpassed\\033[0m\"",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "preinstall": "npx only-allow npm",
    "postinstall": "rm -f tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@phosphor-icons/react": "^2.1.7",
    "@tanstack/react-query": "^5.51.17",
    "@tanstack/react-router": "^1.46.0",
    "framer-motion": "^11.3.19",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.52.1",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/router-devtools": "^1.46.0",
    "@tanstack/router-plugin": "^1.45.13",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.0.5",
    "execa": "^9.4.1",
    "husky": "^9.1.6",
    "jsdom": "^24.1.1",
    "listr2": "^8.2.5",
    "msw": "^2.3.5",
    "tsx": "^4.19.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.5"
  }
}
